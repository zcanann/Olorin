    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'squalr-cli'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=squalr-cli",
                    "--package=squalr-cli"
                ],
                "filter": {
                    "name": "squalr-cli",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Release executable 'squalr-cli'",
